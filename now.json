{
  "version": 2,
  "name": "test-api",
  "builds": [
    {
      "src": "/api/**/*.ts",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "/api/index.ts",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "/api/hello",
      "dest": "/api/index.ts",
      "methods": [
        "GET"
      ]
    }
  ]
}